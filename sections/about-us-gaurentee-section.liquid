      <section class="absolutely-guaranteed container-width">
        <div class="box">
          <div class="iconBox">
            <img src="{{ section.settings.slide_image1 | img_url: 'master'}}" alt="">
          </div>
          <div class="textBox">
            <h2>{{section.settings.heading}}</h2>
            <p>{{section.settings.cofoundertext}}</p>
            <a href="{{section.settings.linktest12}}">{{section.settings.linktest}}</a>
          </div>
        </div>
      </section>

{% schema %}
  {
    "name": "Guaranteed Section",
    "settings": [
				  {
                      "id": "slide_image1",
                      "type": "image_picker",
                      "label": "Guarantee's Image"
                  },                 
                  {
                      "type":"html",
                      "label":"heading",
                      "id":"heading"
                  },
                  {
                      "type":"html",
                      "label":"Description",
                      "id":"cofoundertext"
                  },
                  {
                      "type":"text",
                      "label":"Link Text",
                      "id":"linktest"
                  },
                  {
                      "type":"text",
                      "label":"Link Url",
                      "id":"linktest12"
                  }
    ]
  }
{% endschema %}